{
    "compilerOptions": {
        "module": "commonjs",
        "jsx": "react",
        "watch": true,
        // "target": "es5",
        "lib": [
            "es6",
            "dom"
        ],
        "plugins": [
            {
                "name": "typescript-plugin-css-modules"
            }
        ],
        "esModuleInterop": true,
        "target": "ES2020",
        "moduleResolution": "node"
    },
    "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"],
    "include": ["src", "index.d.ts"]
  }